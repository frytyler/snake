{
    "name": "snake",
    "version": "0.0.1",
    "description": "Old school snake game using RxJs and all the javascripts",
    "main": "index.js",
    "author": "Tyler Fry",
    "license": "MIT",
    "scripts": {
        "start": "webpack-dev-server --progress --inline --hot",
        "test": "jest --colors",
        "test:watch": "yarn test -- --watchAll"
    },
    "jest": {
        "notify": false,
        "rootDir": "./",
        "testRegex": ".(spec).js[x]?$",
        "moduleFileExtensions": ["js", "json"],
        "moduleNameMapper": {
            "app/(.*)": "<rootDir>/app/$1",
            "\\.(css|scss)$": "identity-obj-proxy"
        },
        "testPathIgnorePatterns": ["/node_modules/"]
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-register": "^6.24.1",
        "path": "^0.12.7",
        "ramda": "^0.24.1",
        "rxjs": "^5.4.2"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "chai": "^4.1.0",
        "jest": "^20.0.4",
        "jest-cli": "^20.0.4",
        "webpack": "^3.4.1",
        "webpack-dev-server": "^2.6.1"
    }
}
